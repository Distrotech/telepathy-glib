[ConnectionManager]
Name = gabble
BusName = org.freedesktop.Telepathy.ConnectionManager.gabble
ObjectPath = /org/freedesktop/Telepathy/ConnectionManager/gabble
Protos = jabber

[Proto jabber]
MandatoryParams = s:account,s:password
OptionalParams  = s:server,q:port,b:old-ssl,s:https-proxy-server,q:https-proxy-port
default-port = 5222
default-old-ssl = 0
